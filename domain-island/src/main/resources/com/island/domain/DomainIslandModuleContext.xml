<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	 <bean id="common" class="com.jcl.core.config.ConfigurationFactoryBean">
		<property name="name" value="common" />
		<property name="type" value="xml" />
		<property name="delimiterParsingDisabled" value="true" />
		<property name="encoding" value="utf8" />
	</bean>
	<bean id="BrokerPpcCeilingLogDAO" class="com.island.domain.dal.BrokerPpcCeilingLogIbatisDAOImpl" />		
	<bean id="testBiz" class="com.island.domain.biz.TestBiz">
		<property name="dependencyConfig">
  			<list>
  				<ref bean="common"/>
  			</list>
  		</property>	
		<property name="testDao" ref="BrokerPpcCeilingLogDAO"></property>
	</bean>

</beans>